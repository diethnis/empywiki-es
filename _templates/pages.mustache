{{>header}}
<div class="results container">
<div id="head" style="border:none;">
  <h1>{{title}}</h1>
</div>
<div id="pages">

{{#has_results}}
  <div id="file-browser">
    <div class="breadcrumb">
      {{{breadcrumb}}}
    </div>
    <ul>
      {{{files_folders}}}
    </ul>
  </div>
{{/has_results}}

{{#no_results}}
  <p id="no-results">
    There are no pages in <strong>{{ref}}</strong>.
  </p>
{{/no_results}}

</div>
</div>

